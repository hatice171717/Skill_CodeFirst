@using Skill_CodeFirst.Models.siniflar
@model List<YETENEKLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}



<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>YETENEK</th>
        <th>YETENEKDEĞER</th>
        <th>SİL</th>
        <th>GÜNCELLE</th>
    </tr>
   
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.ID</td>
            <td>@p.ACIKLAMA</td>
            <td>@p.DEGER</td>
            <td><a  href="/Admin/YetenekSil/@p.ID" class="btn btn-danger">SİL</a></td>
            <td><a href="/Admin/YetenekGetir/@p.ID" class="btn btn-success">GÜNCELLE</a></td>
        </tr>
    }

</table>